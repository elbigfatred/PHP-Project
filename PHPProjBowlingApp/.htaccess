RewriteEngine on

# Get games by round
RewriteRule ^scorekeeper/games$ MiddleWare/ScoreKeeper.php [NC]

# Game operations (start, submit, reset)
RewriteRule ^scorekeeper/games/([0-9]+)$ MiddleWare/ScoreKeeper.php?gameId=$1 [NC]
RewriteRule ^scorekeeper/games/([0-9]+)/([a-zA-Z]+)$ MiddleWare/ScoreKeeper.php?gameId=$1&action=$2 [NC]